<% @churches.each do |church| %>
  <div class="booyah-box col-xs-10 col-xs-offset-1">
    <h1><%= church.name %></h1>
    <i><%= church.address %></i><br /><br />
    <p><%= church.description %></p><br />
  </div>

<% end %>

<div class="col-xs-10 col-xs-offset-1" style="text-align: center;">
    <%= paginate @churches %>
</div>